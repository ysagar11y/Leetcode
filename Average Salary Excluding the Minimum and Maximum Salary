class Solution {
    public double average(int[] salary) {
        int min=salary[0],max=salary[0];
        double sum=0;
        for(int i=0;i<salary.length;i++){
            min=Math.min(min,salary[i]);
            max=Math.max(max,salary[i]);
            sum+=salary[i];
        }
        sum=sum-(min+max);
        return (sum/(salary.length-2));
    }
}
